{
    "name": "iluminar/fluent-facebook",
    "description": "A laravel 5 package for reading and writing to facebook graph object with ease in laravelish syntax",
    "keywords": ["laravel", "facebook", "graph-api", "facebook-graph-api"],
    "authors": [
        {
            "name": "nhasnayeen",
            "email": "nehal.hasnayeen@gagagugu.com"
        }
    ],
    "type": "library",
    "require": {
        "illuminate/support": "~5.1",
        "laravel/framework": "5.*",
        "php" : "~5.5|~7.0",
        "symfony/http-foundation": "3.1.*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Iluminar\\Fluent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iluminar\\FluentFacebook\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
